<!DOCTYPE html>
<html lang="en">
 <head>
  <meta charset="UTF-8">
  <meta name="Generator" content="EditPlus®">
  <meta name="Author" content="">
  <meta name="Keywords" content="">
  <meta name="Description" content="">
  <title>Document</title>
  <script src="../echarts.min.js"></script>
 </head>
 <body>
  <div id="main" style="width:500px;height:500px"></div>
  <script type="text/javascript">
	var mychart=echarts.init(document.getElementById("main"));
	var option={
		title:{
			text:"多折线图学习"
		},
		tooltip:{
			trigger:'axis'
		},
		legend:{
			data:['line1','line2','line3']
		},
		toolbox:{
			feature:{
				saveAsImage:{}
			}
		},
		xAxis:{
			type:'category',
			data:['o1','o2','o3'],
			boundaryGap:false
		},
		yAxis:{
			type:'value'
		},
		series:[
		{
			name:'line1',
			type:'line',
			data:[1,2,3],
			stack:'Total',
			areaStyle:{}
		},
		{
			name:'line2',
			type:'line',
			data:[2,1,3],
			stack:'Total',
			areaStyle:{}
		},
		{
			name:"line3",
			type:"line",
			data:[3,1,2],
			stack:"Total",
			areaStyle:{}
		}
		]
	};
	mychart.setOption(option);
  </script>
 </body>
</html>
